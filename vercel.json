{
  "name": "lenny-ai",
  "framework": "vite",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "alias": ["lenny-ai.vercel.app"],
  "github": {
    "silent": false,
    "autoAlias": true,
    "enabled": true,
    "deployOnPush": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-App-Name",
          "value": "Lenny AI"
        },
        {
          "key": "X-Organization-Name",
          "value": "AI Doctor Network"
        }
      ]
    }
  ]
}